# Copy this file to .env and fill in your values
# All API calls go through serverless functions - no VITE_ variables needed

# Tavus API key (get from https://platform.tavus.io)
TAVUS_API_KEY=your_tavus_api_key_here

# Replica ID (get from Tavus dashboard)
REPLICA_ID=your_replica_id_here

# Persona IDs for different interview types
LEET_CODE_PERSONA_ID=your_leetcode_persona_id_here
GENERAL_RECRUITER_ID=your_general_recruiter_persona_id_here

# OpenAI API key (get from https://platform.openai.com)
# Required for AI-generated feedback feature
OPENAI_API_KEY=your_openai_api_key_here

# AWS S3 Configuration for Interview Recordings (Optional)
# If these are not set, recordings will be disabled but the app will still work
# See documentation: https://docs.tavus.io/sections/conversational-video-interface/quickstart/conversation-recordings

# AWS IAM Role ARN for Tavus to access your S3 bucket
# Format: arn:aws:iam::YOUR_ACCOUNT_ID:role/TavusRecordingRole
AWS_ASSUME_ROLE_ARN=

# S3 bucket details
S3_BUCKET_NAME=
S3_BUCKET_REGION=us-east-1

# AWS credentials for presigned URL generation
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
