# Copy this file to .env and fill in your values
# All API calls go through serverless functions - no VITE_ variables needed

# Tavus API key (get from https://platform.tavus.io)
TAVUS_API_KEY=your_tavus_api_key_here

# Replica ID (get from Tavus dashboard)
REPLICA_ID=your_replica_id_here

# Persona IDs for different interview types
LEET_CODE_PERSONA_ID=your_leetcode_persona_id_here
GENERAL_RECRUITER_ID=your_general_recruiter_persona_id_here

# OpenAI API key (get from https://platform.openai.com)
# Required for AI-generated feedback feature
OPENAI_API_KEY=your_openai_api_key_here
